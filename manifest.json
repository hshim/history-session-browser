{
  "name": "History",
  "version": "0.0",
  "manifest_version": 2,
  "description": "Browse history by tasks!",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "chrome://favicon/",
    "tabs",
  	"history"
  ]
}
